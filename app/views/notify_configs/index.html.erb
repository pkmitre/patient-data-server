<h1> <%= @title %> </h1>


<table class="table">
  <tr>
    <th>User</th>
    <th>Interval</th>
    <th>Action</th>
    <th>Alert out1</th>
    <th>Alert out2</th>
    <th>Alert out3</th>
    <th>Alert out4</th>
   <th></th>
    <th></th>
