<% @notify_configs.each do |notify_config| %>
  <tr>
    <td><%= notify_config.user %></td>
    <td><%= notify_config.interval %></td>
    <td><%= notify_config.action %></td>
    <td><%= notify_config.alert_out1 %></td>
    <td><%= notify_config.alert_out2 %></td>
    <td><%= notify_config.alert_out3 %></td>
    <td><%= notify_config.alert_out4 %></td>
   <td><%= link_to 'Show', notify_config %></td>
    <td><%= link_to 'Edit', edit_notify_config_path(notify_config) %></td>
    <td><%= link_to 'Destroy', notify_config, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<p>
